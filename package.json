  },
  "bugs": {
    "url": "https://github.com/heminsatya/polaris-slideshow/issues"
  },
  "devDependencies": {
    "polaris-core": "^0.9.9",
    "polaris-colors": "^1.3.2",
    "sass": "^1.56.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "ts-loader": "^9.4.1",
    "inspectpack": "^4.7.1"
